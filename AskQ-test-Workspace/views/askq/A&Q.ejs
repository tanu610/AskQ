<!DOCTYPE html>
<html>
<head>
  <title>Index Page</title>
  <!-- css link -->
  <link rel="stylesheet" href="/css/index.css">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

  <!-- fontowsome-->
  <script src="https://kit.fontawesome.com/906e9173fa.js" crossorigin="anonymous"></script>

</head>
<body>

  <div class="b_color">
    <header>
      <div><h1>AskQ</h1></div>
    </header>
  </div>
  
  <!-- Navigation -->
  
  <nav class="navbar navbar-expand-lg sticky-top ">
    <a class="navbar-brand" href="#"></a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <!-- left side-->
      <ul class="mr-auto left_nav navbar-nav">
        <li class="nav-item active">
          <a class="nav-link" href="/AskQs">HOME <span class="sr-only">(current)</span></a>
        </li>
        <li class="nav-item ">
          <a class="nav-link" href="/AskQs/A&Q">A&Q</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/AskQs/topic">Topic</a>
        </li>
      </ul>
      <!--Right side-->
      <ul class="ml-auto right_nav navbar-nav">
        <li class="nav-item ask_b active">
          <a class="nav-link btn btn-primary btn-sm" href="/AskQs/new">Ask</a>
        </li>
        <form class="form-inline">
          <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
          <button class="btn bd-search my-2 my-sm-0" type="submit">Search</button>
        </form>
        <li class="nav-item dropdown">
          <a class="nav-link " href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            <i class="fas fa-user"></i>
          </a>
          <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
            <a class="dropdown-item" href="/logout">logout</a>
          </div>
        </li>
        <li class="nav-item pt-2"><%=nae%></li>
      </ul>
    </div>
  </nav>

  
  
  <!-- Main-Body -->
  <div class="container-fluid">
    <div class="row">
    
      <!--left sidebar-->
      

      <!--main body-->
      <div class="col-lg-8 mt-2 pl-5 pr-4 main">
      <h1>All the Ques asked by you</h1>
        <% AskQs.reverse().forEach(function(askq){ %>
        <div class="post">
          <div class="post_in">
            <h4>Topic: <%= askq.tags%></h4>
            <h4><%= askq.QuesH %></h4>
            <p>
              <em>Submitted By <a href="/users/<%= askq.author.id %>"><%= askq.author.username %></a></em>
            </p>
            <p><%= askq.date %></p>
            <p><%= askq.description %></p>
            <p><a href="/AskQs/<%= askq._id %>" class="btn btn-primary">View Q</a></p>
          </div>
        </div>
        <% }); %> 
      </div>


      <!--right sidebar-->
    </div>



    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>


  </body>
  </html>













